# .env.example - HealthLink Configuration Template
# Instructions: Copy this to a new file named .env and fill in your actual values.

# --- Database (MySQL) ---
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=your_database_password
DB_NAME=healthlink

# --- Redis ---
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=your_redis_password

# --- MinIO (S3-compatible Object Storage) ---
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=your_minio_access_key
MINIO_SECRET_KEY=your_minio_secret_key
MINIO_SECURE=false
MINIO_DEFAULT_BUCKET=healthlink

# --- Milvus (Vector Database) ---
MILVUS_HOST=localhost
MILVUS_PORT=19530
MILVUS_DB_NAME=healthlink_db

# --- LLM API Keys & Models ---
MODEL_KEY="sk-..."
EMBEDDING_MODEL_KEY="sk-..." # IMPORTANT: This key may be missing from your .env file
EMBEDDING_MODEL="multimodal-embedding-v1"
EMBEDDING_MODEL_URL="https://dashscope.aliyuncs.com/api/v1/services/embeddings/multimodal-embedding/multimodal-embedding"
TEXT_EMBEDDING_MODEL="text-embedding-v4"
MODEL_URL="https://dashscope.aliyuncs.com/compatible-mode/v1"
MODEL_NAME="qwen3-max" # NOTE: The key is MODEL_NAME, not MODE_NAME

# --- AI Agent Settings ---
TEMP_MEMORY_SIZE=10

# --- Project Settings ---
PROJECT_NAME="HealthLink AI Assistant"

# --- JWT (JSON Web Token) Settings ---
JWT_SECRET_KEY="your_super_secret_random_string_for_jwt"
JWT_ALGORITHM="HS256"
ACCESS_TOKEN_EXPIRE_MINUTES=10080
JWT_LOGIN_SUBJECT="HealthLink"